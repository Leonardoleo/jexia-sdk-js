{
  "name": "jexia-sdk-js",
  "version": "0.0.0",
  "description": "jexia javascript sdk",
  "main": "index.js",
  "scripts": {
    "cleanup": "rimraf dist coverage docs .nyc_output",
    "cleanup:full": "npm run cleanup && rimraf node_modules",
    "lint": "tslint \"src/**/*.ts\" \"spec/**/*.ts\"",
    "transpile": "npm run cleanup && tsc",
    "test": "npm run transpile && nyc jasmine",
    "test:coverage": "npm run test && nyc report --reporter=html && opn coverage/index.html",
    "tsnode": "ts-node",
    "compodoc": "compodoc",
    "docs": "rimraf docs && npm run compodoc -- src/api -p tsconfig.json --disablePrivateOrInternalSupport --disableCoverage --disableSourceCode --name \"Jexia Javascript SDK\" --output docs --includes extra_docs",
    "bundle": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jexia-com/jexia-sdk-js.git"
  },
  "author": "Mihai Coman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jexia-com/jexia-sdk-js/issues"
  },
  "homepage": "https://github.com/jexia-com/jexia-sdk-js#readme",
  "devDependencies": {
    "@compodoc/compodoc": "^1.0.0-beta.14",
    "@types/jasmine": "2.5.47",
    "@types/node": "^7.0.18",
    "@types/node-fetch": "1.6.7",
    "@types/ws": "^3.0.2",
    "jasmine": "2.6.0",
    "node-fetch": "1.6.3",
    "nyc": "^10.3.2",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.0",
    "webpack": "^2.4.1",
    "webpack-uglify-js-plugin": "^1.1.9",
    "ws": "^3.1.0"
  },
  "dependencies": {
    "node-fetch": "^1.6.3"
  }
}
