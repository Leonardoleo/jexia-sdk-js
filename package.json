{
  "name": "jexia-sdk-js",
  "version": "0.0.0",
  "description": "jexia javascript sdk",
  "main": "index.js",
  "scripts": {
    "cleanup": "rimraf dist coverage node_modules docs",
    "lint": "tslint \"src/**/*.ts\" \"spec/**/*.ts\"",
    "transpile": "rimraf dist coverage && tsc",
    "test": "npm run transpile && nyc jasmine",
    "test:coverage": "npm run transpile && nyc report --reporter=html && opn coverage/index.html",
    "typedoc": "typedoc",
    "docs": "rimraf docs && npm run typedoc -- --out ./docs --exclude \"**/*Test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jexia-com/jexia-sdk-js.git"
  },
  "author": "Mihai Coman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jexia-com/jexia-sdk-js/issues"
  },
  "homepage": "https://github.com/jexia-com/jexia-sdk-js#readme",
  "devDependencies": {
    "@types/bluebird": "3.5.3",
    "@types/jasmine": "2.5.47",
    "@types/node": "^7.0.18",
    "@types/node-fetch": "1.6.7",
    "jasmine": "2.6.0",
    "node-fetch": "1.6.3",
    "nyc": "^10.3.2",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.0",
    "typedoc": "^0.7.1",
    "webpack": "^2.4.1",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "node-fetch": "^1.6.3"
  }
}
