{
  "compilerOptions": {
    "strict": false,
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
      "es2017",
      "dom"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": false,
    "outDir": "./.temp",
    "alwaysStrict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "pretty": true,
    "declaration": false,
    "removeComments": true,
    "baseUrl": ".",
    "paths": {
      "jexia-sdk-js/*": [
        "src/*"
      ]
    }
  }
}
